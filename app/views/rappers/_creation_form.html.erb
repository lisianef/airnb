<%= simple_form_for @booking, url: rapper_bookings_path(@rapper) do |f| %>
  <%= f.input :starting_on, as: :string, required: false, input_html: {class: "datepicker"} %>
  <%= f.input :ending_on, as: :string, required: false, input_html: {class: "datepicker"} %>
  <%= f.button :submit, "Go", class: "btn btn-primary" %>
<% end %>
